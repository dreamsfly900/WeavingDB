<?xml version="1.0" encoding="utf-8"?>
<!--服务设置-->
<Setting>
  <!--服务名-->
  <ServiceName>WeavingDB</ServiceName>
  <!--显示名-->
  <DisplayName>WeavingDB</DisplayName>
  <!--服务描述-->
  <Description>雷达-遥感-等值面图数据库</Description>
  <!--最大占用内存。超过最大占用时，整个服务进程将会重启，以释放资源。默认8096M-->
  <MaxMemory>8096</MaxMemory>
  <!--最大线程数。超过最大占用时，整个服务进程将会重启，以释放资源。默认1000个-->
  <MaxThread>1000</MaxThread>
  <!--最大句柄数。超过最大占用时，整个服务进程将会重启，以释放资源。默认10000个-->
  <MaxHandle>10000</MaxHandle>
  <!--自动重启时间。到达自动重启时间时，整个服务进程将会重启，以释放资源。默认0分，表示无限-->
  <AutoRestart>0</AutoRestart>
  <!--看门狗，保护其它服务，每分钟检查一次。多个服务名逗号分隔-->
  <WatchDog></WatchDog>
</Setting>